{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ],
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "https://newdomain.com/$1",
        "permanent": true
      }
    ]
  }